pipeline {
    agent any
      tools {
            maven 'apache-maven-3.8.7'
        }
    stages {
        stage('Checkout GIT') {
            steps {
                checkout([
                    $class: 'GitSCM',
                    branches: [[name: 'main']],
                    userRemoteConfigs: [[url: 'https://github.com/IyadhJm/miniProjet.git']]
                ])
            }
        }



        stage('MVN CLEAN') {
            steps {
                withMaven(maven: 'apache-maven-3.8.7') {
 withMaven(maven: 'apache-maven-3.8.7') {
                    bat 'mvn --batch-mode -V -U -e clean deploy -Dsurefire.useFile=false'                }
            }
        }


    }
}