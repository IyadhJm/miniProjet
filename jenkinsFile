pipeline {
    agent any
    stages {
        stage('Checkout GIT') {
            steps {
                checkout([
                    $class: 'GitSCM',
                    branches: [[name: 'main']],
                    userRemoteConfigs: [[url: 'https://github.com/IyadhJm/miniProjet.git']]
                ])
            }
        }

        stage('MVN CLEAN') {
            steps {
              {
              bat 'mvn clean compile'

           }
         }
        }
}
}


